{
  "name": "node-rdap",
  "version": "0.3.3",
  "type": "module",
  "main": "./dist/tsc/client.js",
  "types": "./dist/tsc/client.d.ts",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dcoffey-zengenti/node-rdap"
  },
  "author": "Derry coffey <d.coffey@zengenti.com>",
  "license": "MIT",
  "keywords": [
    "rdap",
    "registration data access protocol",
    "domain lookup",
    "ip address lookup",
    "autnum lookup",
    "dns lookup"
  ],
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rimraf dist build package",
    "ts-node": "ts-node",
    "cli": "node --experimental-specifier-resolution=node --experimental-modules dist/tsc/cli.js",
    "build": "tsc -p tsconfig.json",
    "build:cli": "yarn build && yarn cli",
    "build-all": "yarn clean && yarn build"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/punycode": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "got": "^12.5.3",
    "ip-matching": "^2.1.2",
    "node-cache": "^5.1.2",
    "punycode": "^2.1.1"
  }
}
